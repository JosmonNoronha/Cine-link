# Copy this to .env and fill in your values

# Development API URL (optional - will auto-detect if not set)
EXPO_PUBLIC_API_BASE_URL=http://localhost:5001/api

# Production API URL (required for production builds)
EXPO_PUBLIC_PRODUCTION_API_URL=https://your-backend.railway.app/api

# Existing API keys
EXPO_PUBLIC_OMDB_API_KEY=your_omdb_key
EXPO_PUBLIC_YOUTUBE_API_KEY=your_youtube_key

# Firebase config (existing)
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_key
# ... other firebase config
